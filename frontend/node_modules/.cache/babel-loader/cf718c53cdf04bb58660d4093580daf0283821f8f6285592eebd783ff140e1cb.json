a();\n  },\n\n  methods: {\n    fetchData() {\n      const apiUrl = 'http://localhost:8080/indicadores';\n\n      axios\n        .get(apiUrl)\n        .then((response) => {\n          this.rows = response.data.map((indicador) => ({\n            Indicadores: indicador.codigo,\n            descricao: indicador.descricao, \n            ano: indicador.ano,\n            consumo: indicador.consumo,\n            regiao: indicador.regiao,\n            id: indicador.id\n          }));\n        })\n        .catch((error) => {\n          console.error('Erro ao buscar dados:', error);\n        });\n    },\n\n    redirectToComponent(id) {\n      // Adicione aqui a lógica para navegar para outro componente com base no ID\n      // Por exemplo, usando this.$router.push para direcionar para uma rota específica\n      // this.$router.push({ name: 'NomeDaRota', params: { id: id } });\n    },\n\n  },\n};\n</script>\n\n"],"mappings":"AAgBA,OAAAA,KAAA;AAEA;EACAC,KAAA;IACA;MACAC,OAAA;MACAC,IAAA;MACAC,OAAA,GAEA;IACA;EACA;EAEAC,QAAA;IACA,KAAAC,SAAA;EACA;EAEAC,OAAA;IACAD,UAAA;MACA,MAAAE,MAAA;MAEAR,KAAA,CACAS,GAAA,CAAAD,MAAA,EACAE,IAAA,CAAAC,QAAA;QACA,KAAAR,IAAA,GAAAQ,QAAA,CAAAV,IAAA,CAAAW,GAAA,CAAAC,SAAA;UACAC,WAAA,EAAAD,SAAA,CAAAE,MAAA;UACAC,SAAA,EAAAH,SAAA,CAAAG,SAAA;UACAC,GAAA,EAAAJ,SAAA,CAAAI,GAAA;UACAC,OAAA,EAAAL,SAAA,CAAAK,OAAA;UACAC,MAAA,EAAAN,SAAA,CAAAM,MAAA;UACAC,EAAA,EAAAP,SAAA,CAAAO;QACA;MACA,GACAC,KAAA,CAAAC,KAAA;QACAC,OAAA,CAAAD,KAAA,0BAAAA,KAAA;MACA;IACA;IAEAE,oBAAAJ,EAAA;MACA;MACA;MACA;IAAA;EAGA;AACA"},"metadata":{},"sourceType":"module","externalDependencies":[]}